  },
  "repository": "https://github.com/TheUnnamedOrganisation/RocketBot2-APK",
  "keywords": [
    "RocketBot2"
  ],
  "author": "RocketBot2 Team",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.13",
    "electron-builder": "^8.6.0",
    "electron-packager": "^8.3.0"
  },
  "build": {
    "appId": "RocketBot2",
    "productName": "RocketBot2",
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/TheUnnamedOrganisation/RocketBot/master/RocketBot2/Resources/Ico.ico"
    }
